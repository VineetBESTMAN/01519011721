// src/components/Pagination.js
import React from 'react';
import { Pagination } from '@mui/material';

const CustomPagination = ({ page, count, onChange }) => {
  return (
    <Pagination
      count={count}
      page={page}
      onChange={onChange}
      color="primary"
      showFirstButton
      showLastButton
    />
  );
};

export default CustomPagination;
